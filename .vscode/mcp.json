{
	"servers": {
		"zendeskapi-mcp-server": {
			"type": "stdio",
			"command": "npm",
			"args": [
				"run",
				"dev"
			],
			"cwd": "${workspaceFolder}",
			"dev": {
				"watch": "src/**/*.js",
				"debug":{
					"command": "npm",
					"args": [
						"run",
						"inspect"
					]
				}
			}
		}
	},
	"inputs": []
}